<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="rpn_calculator" name="calculator.domain.Result">
    <id column="id" unsaved-value="null" name="id">
      <generator class="native"/>
    </id>
    <property name="answer" column="response"/>
    <property name="request" column="request"/>
    <property name="resultCode" column="result_code"/>
    <property name="resultMsg" column="result_msg"/>
  </class>
</hibernate-mapping>
